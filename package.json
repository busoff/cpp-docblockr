{
    "name": "cpp-docblockr",
    "displayName": "cpp-docblockr",
    "description": "cpp doc block generator",
    "version": "0.0.1",
    "publisher": "busoff",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:cpp",
        "onLanguage:c"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "eslint": "^4.11.0",
        "xregexp": "^4.2.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
